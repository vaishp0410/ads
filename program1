#include <bits/stdc++.h>
using namespace std;

int main(){
    
    string a, b, ans;
    cin>>a>>b;
    
    int n=a.length(), m=b.length();
    string temp=a;
    
    sort(temp.begin(), temp.end());
    if(n<m)
    {
        reverse(temp.begin(), temp.end());
        ans=temp;
    }
    
    else
    {
        a=temp;
        
        for(int i=0; i<n; i++){
            temp=a;
            for(int j=i+1; j<n; j++){
                swap(temp[i], temp[j]);
                
                if(temp<=b){
                    a=temp;
                }
            }
        }
        ans=temp;
    }
    
    cout<<ans;
    
    return 0; 
}
